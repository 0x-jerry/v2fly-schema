{
  "name": "@0x-jerry/v2ray-schema",
  "version": "0.2.6",
  "author": "Jerry Wang<x.jerry.wang@gmail.com>",
  "repository": "https://github.com/0x-jerry/v2ray-config-json-schema.git",
  "types": "src/index.ts",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:deno && npm run build:node",
    "build:deno": "rm -rf deno && ts-node scripts/build-deno.ts",
    "build:node": "typescript-json-schema src/index.ts IV2Ray -o V2Ray.config.schema.json && tsup src/index.ts",
    "release": "x-release"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@0x-jerry/x-release": "0.3.4-0",
    "@babel/core": "^7.17.10",
    "@babel/plugin-syntax-typescript": "^7.17.10",
    "@types/babel__core": "^7.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.31",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.4",
    "typescript-json-schema": "^0.53.0"
  }
}