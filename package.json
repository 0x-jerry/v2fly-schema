{
  "name": "@0x-jerry/v2ray-schema",
  "version": "0.3.0",
  "author": "Jerry Wang<x.jerry.wang@gmail.com>",
  "repository": "https://github.com/0x-jerry/v2ray-config-json-schema.git",
  "types": "src/index.ts",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "npm run build:deno && npm run build:node",
    "build:deno": "rm -rf deno && tsx scripts/build-deno.ts",
    "build:node": "typescript-json-schema src/index.ts IV2Ray -o V2Ray.config.schema.json && tsup src/index.ts",
    "release": "x-release"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "crawlee": "^3.2.2",
    "puppeteer": "^19.7.2"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "0.3.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "typescript-json-schema": "^0.55.0"
  }
}